def read_f(name):
	ox = []
	oy = []
	with open(name) as f:
		data = f.readlines()
		# print(data)

	for i in range(len(data)):
		line = data[i].split()
		ox.append(float(line[0]))
		oy.append(float(line[1]))
	return ox,oy

]]></content>
